<script language="JavaScript">
		var SetTime = 5;		
		function msg_time() {							ms = Math.floor(SetTime % 60) + " 초 남음";
var msg = "<h2>접속까지 <font color='blue'>" + ms + "</font></h2>";
document.all.ViewTimer.innerHTML = msg;
SetTime--;	
	if (SetTime < 0) {											clearInterval(tid);						
window.location.href = 'https://marpple.shop/kr/camt4372/?page=1';
			}
}
		window.onload = function TimerStart(){ tid=setInterval('msg_time()',1000) };
</script>
